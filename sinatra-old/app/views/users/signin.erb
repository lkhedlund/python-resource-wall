<header>
  <div class="container">
    <div class="intro-text">
      <div class="intro-lead-in">Welcome Back to Nova.</div>
      <div class="intro-heading">Sign-In</div>
    </div>

    <div class="row" style="margin-bottom: 20px">
      <form class="col-md-1 col-md-offset-4" method="post" action="/users/signin/test">
        <input type="hidden" name="user_id" value="1" />
        <button class="btn btn-default">User ID: 1</button>
      </form>

      <form class="col-md-1" method="post" action="/users/signin/test">
        <input type="hidden" name="user_id" value="2" />
        <button class="btn btn-default">User ID: 2</button>
      </form>

      <form class="col-md-1" method="post" action="/users/signin/test">
        <input type="hidden" name="user_id" value="3" />
        <button class="btn btn-default">User ID: 3</button>
      </form>

      <form class="col-md-1" method="post" action="/users/signin/test">
        <input type="hidden" name="user_id" value="4" />
        <button class="btn btn-default">User ID: 4</button>
      </form>
    </div>

    <div class="row ltgray">
      <div class="col-md-4 col-md-offset-4">
        <form role="form" method="post" action="/users/signin">
          <div class="form-group">
            <label for="email">Email: </label>
            <input class="form-control" id="email" name="email" />
          </div>
          <div class="form-group">
            <label for="password">Password: </label>
            <input class="form-control" type="password" id="password" name="password" />
          </div>
          <div>
            <button class="btn btn-default">Sign in</button>
          </div>
        </form>
      </div>
    </div>

  </div>
</header>